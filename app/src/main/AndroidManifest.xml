<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="team3.budgee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Setup"
            android:label="@string/title_activity_setup" />
        <activity android:name=".activity.ExpressSetup" />
        <activity
            android:name=".activity.StandardSetup"
            android:label="@string/title_activity_standard_setup" />
        <activity
            android:name=".activity.AdvancedSetup"
            android:label="@string/title_activity_advanced_setup"></activity>
    </application>

</manifest>